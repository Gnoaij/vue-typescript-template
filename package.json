{
  "name": "vue-typescript-template",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:css": "stylelint **/*.{css,scss,vue} --ignore-path .gitignore",
    "lint:fix": "yarn lint:js --fix && yarn lint:css --fix",
    "lint:staged": "lint-staged --concurrent false",
    "format": "prettier . --write --ignore-path .gitignore && yarn lint:css --fix",
    "preinstall": "node ./scripts/preinstall.js",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^0.27.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.16",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/json": "^2.1.81",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.0.6",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "stylelint": "^14.9.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "terser": "^5.14.2",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.10.0",
    "unplugin-icons": "^0.14.7",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^0.39.0"
  }
}
