{
  "extends": [
    "stylelint-config-standard-scss",
    "stylelint-config-recess-order",
    "stylelint-config-prettier-scss",
    "stylelint-config-html/vue"
  ],
  "rules": {
    "color-hex-length": null,
    "custom-property-empty-line-before": null,
    "value-no-vendor-prefix": [
      true,
      {
        "ignoreValues": ["box"]
      }
    ]
  }
}
